{
	"$schema": "https://mintlify.com/docs.json",
	"theme": "mint",
	"name": "Reactive Resume",
	"favicon": "/favicon.svg",
	"description": "A one-of-a-kind resume builder that keeps your privacy in mind. Completely secure, customizable, portable, open-source and free forever. Try it out today!",
	"appearance": {
		"default": "dark"
	},
	"fonts": {
		"heading": {
			"family": "IBM Plex Sans"
		},
		"body": {
			"family": "IBM Plex Sans"
		}
	},
	"colors": {
		"primary": "#16A34A",
		"light": "#07C983",
		"dark": "#15803D"
	},
	"navigation": {
		"tabs": [
			{
				"tab": "Documentation",
				"groups": [
					{
						"group": "Getting Started",
						"pages": ["getting-started/index", "getting-started/quickstart"]
					},
					{
						"group": "Contributing",
						"pages": ["contributing/architecture", "contributing/development"]
					},
					{
						"group": "Legal",
						"pages": ["legal/license", "legal/privacy-policy", "legal/terms-of-service"]
					}
				]
			},
			{
				"tab": "API Reference",
				"openapi": "/spec.json"
			}
		],
		"global": {
			"anchors": [
				{
					"icon": "link",
					"anchor": "Reactive Resume",
					"href": "https://rxresu.me"
				},
				{
					"icon": "github",
					"anchor": "Source Code",
					"href": "https://github.com/AmruthPillai/Reactive-Resume"
				}
			]
		}
	},
	"logo": {
		"light": "/logo/light.svg",
		"dark": "/logo/dark.svg"
	},
	"navbar": {
		"links": [
			{
				"label": "Support",
				"href": "mailto:hello@amruthpillai.com"
			}
		],
		"primary": {
			"type": "button",
			"label": "Get Started",
			"href": "https://rxresu.me"
		}
	},
	"contextual": {
		"options": ["copy", "view"]
	},
	"footer": {
		"socials": {
			"x": "https://x.com/KingOKings",
			"github": "https://github.com/AmruthPillai",
			"linkedin": "https://linkedin.com/in/amruthpillai"
		}
	}
}
